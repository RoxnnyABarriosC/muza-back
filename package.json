  "scripts": {
    "build": "nest build",
    "start": "node dist/main",
    "start:doc": "npx @compodoc/compodoc -p tsconfig.build.json -s ",
    "start:dev": "nest start -w",
    "start:dev:check": "nest start --type-check -w",
    "start:dev:hmr": "nest build --webpack --webpackPath webpack-hmr.config.js -w",
    "start:debug": "nest start --type-check --debug -w",
    "start:repl": "nest start --entryFile repl",
    "format": "prettier-eslint --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ts:check": "tsc -p tsconfig.build.json --noEmit",
    "pre:build": "rimraf dist",
    "pre:commit": "lint-staged",
    "pre:check": "pnpm ts:check && pnpm lint",
    "prepare": "npx husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:cov:check": "vitest run --coverage -- --CHECK",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "migrate": "npx typeorm migration:run -d dist/config/db/data-source.js",
    "seed": "npx typeorm migration:run -d dist/config/db/data-source.js -- --SEED",
    "migration:create": "npx typeorm migration:create",
    "m:c": "npx typeorm migration:create",
    "migration:revert": "npx typeorm migration:revert -d dist/config/db/data-source.js",
    "m:r": "npx typeorm migration:revert -d dist/config/db/data-source.js"
  },
